{
  "expo": {
    "name": "TaskSync",
    "slug": "TaskSync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": ["tasksync", "exp+tasksync"],
    "splash": {
      "image": "./assets/splash-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techcollins.TaskSync"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-logo.jpg",
        "backgroundColor": "#FFFFFF"  
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.techcollins.TaskSync"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "f4c57632-7c56-4890-a983-d681aa731ac8"
      }
    }
  }
}